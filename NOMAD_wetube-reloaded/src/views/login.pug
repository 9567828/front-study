extends base

block content
  if errorMessage
    span=errorMessage
  form(method="POST")
    input(name="username" type="text" placeholder="아이디입력" required)
    input(name="password" type="password" placeholder="비번입력" required)
    input(type="submit", value="login")
    br
    a(href="/users/github/start") 깃헙로그인 &rarr;
  hr
  div
    span 계정이 없나요? 
    a(href="/join") 가입하기 &rarr; 
