mixin video(info)
  a(href=`/videos/${info.id}`).video-mixin
    div.video-mixin_thumb
    div.video-mixin-data
      span.video-mixin-title=info.title
      div.video-mixin-meta
        span #{info.owner.name || user.username} • 
        span #{info.meta.views}회 • 
        span 좋아요 #{info.likes.length}
