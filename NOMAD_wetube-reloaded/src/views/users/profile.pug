extends ../base
include ../mixins/video

block content
  div.profile-data
    if !user.avatarUrl
      span 😂
    else 
      img(src=`/${user.avatarUrl}`, alt="프로필사진").avatar
    h3=user.username
  div.video-grid
    each video in user.videos
      +video(video)
    else
      span.empty-message 영상이 없습니다.